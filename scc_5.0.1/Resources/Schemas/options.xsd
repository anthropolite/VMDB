  <!-- List of Installed SCAP Content Streams -->
  <xs:element name="contents">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="content" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- Installed SCAP Content Stream -->
  <xs:element name="content">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="stream"/>
        <xs:choice>
          <!-- SCAP v.1.0/1.1 Content Stream Properties -->
          <xs:sequence>
            <xs:element ref="selectedProfile"/>
            <xs:element ref="profiles"/>
            <xs:element ref="lastModifiedXCCDF"/>
            <xs:element ref="lastModifiedOVAL"/>
            <xs:element ref="certificates"/>
			<xs:choice>
              <!-- SCAP with OVAL Patches Content -->
              <xs:sequence>
                <xs:element ref="lastModifiedPatches"/>
                <xs:element ref="lastModifiedCPEOVAL"/>
                <xs:element ref="lastModifiedCPEDictionary"/>
                <xs:element ref="installDate"/>
                <xs:element ref="version"/>
                <xs:element ref="ovalVersion"/>
                <xs:element ref="xccdf"/>
                <xs:element ref="oval"/>
                <xs:element ref="date"/>
                <xs:element ref="patchModifiedDate"/>
              </xs:sequence>
              <!-- No OVAL Patches Content -->
              <xs:sequence>
                <xs:element ref="lastModifiedCPEOVAL"/>
                <xs:element ref="lastModifiedCPEDictionary"/>
                <xs:element ref="installDate"/>
                <xs:element ref="version"/>
                <xs:element ref="ovalVersion"/>
                <xs:element ref="xccdf"/>
                <xs:element ref="oval"/>
                <xs:element ref="date"/>
              </xs:sequence>
            </xs:choice>
          </xs:sequence>
          <!-- SCAP v.1.2 Content Stream Properties -->
          <xs:sequence>
            <xs:element ref="filepath"/>
            <xs:element ref="selectedProfile"/>
            <xs:element ref="profiles"/>
            <xs:element ref="lastModifiedSCAP12"/>
            <xs:element ref="installDate"/>
            <xs:element ref="version"/>
            <xs:element ref="ovalVersion"/>
            <xs:element ref="xccdf"/>
            <xs:element ref="oval"/>
            <xs:element ref="date"/>
			<xs:element ref="certificates"/>
          </xs:sequence>
        </xs:choice>
        <xs:element ref="xmlSchemaValidationDate"/>
        <xs:element ref="xmlSchemaValidationResult"/>
        <xs:element ref="xmldsigValidationDate"/>
        <xs:element ref="xmldsigStatus"/>
        <xs:element ref="enabled"/>
        <xs:element ref="review"/>
        <xs:element ref="resume"/>
        <xs:element ref="resumeFile"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>  
  <!-- List of SCAP Stream Profiles -->
  <xs:element name="profiles">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="profile" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="certificates">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="certificate" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- List of Installed OVAL Content Streams -->
  <xs:element name="ovalContents">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ovalContent" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
