  </xs:element>
  <!-- List of Installed OCIL Content Steams -->
  <xs:element name="ocilContents">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ocilContent" minOccurs="0" maxOccurs="unbounded"/>
